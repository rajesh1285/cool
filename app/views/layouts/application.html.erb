  <!DOCTYPE html>
  <html>
    <head>
      <title>Blogapp</title>
      <%= csrf_meta_tags %>

      <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
      <%= stylesheet_link_tag    'application', 'https://fonts.googleapis.com/css?family=Releway:400,700' %>
      <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    </head>

    <body>
    <div id="sidebar">
      <div id="logo">
        <%=link_to root_path do %>
        <%= image_tag "logo .png"%>
         <%end%>
      </div>
  <ul>
  	<li class="category">website</li>
  	<li><%= link_to"Blogapp",root_path%></li>
  	<li><%= link_to"About" ,about_path%></li>
      <li class="category">social</li>
      <li> <a href="https://www.twitter.com", target="_blank">Twitter</a></li>
      <li> <a href="https://www.google.com", target="_blank">Google</a></li>
      <li><a href="https://www.instagram.com", target="_blank">Instagram</a></li>
      <li><a href="https://www.linkedin.com", target="_blank">Linkedin</a></li>
      <li> <a href="https://www.maropost.com", target="_blank">Maropost</a></li>
  </ul>
  <p class="sign_in">AdminLogin </p>
  </div>
  

    <div id="main_content">
       <div id="header">
       <div class="button">
        <% if current_page?(root_path) %>
            <p>All Posts</p>
        <% elsif current_page?(about_path) %>
          <p>About</p>
        <% else %>
          <%= link_to "Back to All Posts", root_path %>
        <% end %>

           
       
          <button class="button"><%=link_to"newpost",new_post_path%></button>
          <button class="button">Logout</button>
        </div>
       <%flash.each do |name,msg|%>
       <%end%>
    </div>
   <%=yield %>

    </body>
  </html>
