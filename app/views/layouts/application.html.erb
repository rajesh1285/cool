  <!DOCTYPE html>
  <html>
    <head>
      <title>Blogapp</title>
      <%= csrf_meta_tags %>

      <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
      <%= stylesheet_link_tag    'application', 'https://fonts.googleapis.com/css?family=Releway:400,700' %>
      <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    </head>

    <body>
    <div id="sidebar">
      <div id="logo">
        <%=link_to root_path do %>
        <%= image_tag "logo .png"%>
         <%end%>
      </div>
  <ul>
  	<li class="category">website</li>
  	<li><%= link_to"Blogapp",root_path%></li>
  	<li>About</li>
      <li class="category">social</li>
      <li> <a href="https://www.twitter.com">Twitter</a></li>
      <li> <a href="https://www.google.com">Google</a></li>
      <li><a href="https://www.instagram.com">Instagram</a></li>
      <li><a href="https://www.Linkdin.com">Linkdin</a></li>
      <li> <a href="https://www.Email.com">Email</a></li>
  </ul>
  <p class="sign_in">AdminLogin </p>
  </div>
  

    <div id="main_content">
       <div id="header">
           
        <div class="button"><p>Allpost</p>
          <button class="button"><%=link_to"newpost",new_post_path%></button>
          <button class="button">Logout</button>
        </div>
      <%flash.each do |name,msg|%>
       <%end%>
    </div>
   <%=yield %>

    </body>
  </html>
